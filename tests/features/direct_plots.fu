# This program demonstrates the direct plots

fig figure{

	# x vs. y direct plots
	x <- ['i', 'ii', 'iii', 'iv', 'v']
	y <- [1, 2, 3, 4, 5]
	plot(x, y, {color='blue'})

	# this is same as above - inputs are sorted
	# and hence overwritten
	x <- ['i', 'iii', 'ii', 'v', 'iv']
	y <- [1, 3, 2, 5, 4]
	plot(x, y, {color='red'})
}